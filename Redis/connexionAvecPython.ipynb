   "source": [
    "# Connexion à Redis avec Python"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import redis\n",
    "\n",
    "client = redis.Redis(host='localhost', port=6379, db=0)\n",
    "\n",
    "# Vérif de la co\n",
    "try:\n",
    "    pong = client.ping()\n",
    "    print(f\"Connexion réussie : {pong}\")\n",
    "except redis.ConnectionError:\n",
    "    print(\"Erreur de connexion à Redis\")"
   ]
